                x = minX;
            if (device_mouse_x(i) &gt; maxX)
                x = maxX;
        }
        //Vertical movement
        if ( (x &gt; minX-5 &amp;&amp; x &lt; minX+5) || 
             (x &gt; xstart-10 &amp;&amp; x &lt; xstart+10) || 
             (x &gt; maxX-5 &amp;&amp; x &lt; maxX+5) ){
